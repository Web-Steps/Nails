          "_name": "info4",
          "_customHTML": "<section class=\"info4\" group=\"Info\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!--Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\n        <select title=\"Align Content\" name=\"alignContent\">\n            <option value=\"flex-start\">Left</option>\n            <option value=\"center\" selected>Center</option>\n            <option value=\"flex-end\">Right</option>\n        </select>&gt;\n        <input type=\"checkbox\" title=\"Show Top Text\" name=\"showTop\" checked>\n        <input type=\"color\" title=\"Top Text BG\" name=\"topBg\" value=\"#ffffff\" condition=\"showTop\">\n        <input type=\"checkbox\" title=\"Show Bottom Text\" name=\"showBottom\" checked>\n        <input type=\"color\" title=\"Bottom Text BG\" name=\"bottomBg\" value=\"#ffffff\" condition=\"showBottom\">\n        <input type=\"color\" title=\"Borders Color\" name=\"borderColor\" value=\"#b69e59\">\n        <input type=\"text\" title=\"Link\" name=\"link\" value=\"tel:+10000000000\">\n\n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#ffffff\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"container\">\n        <div class=\"mbr-row\">\n            <div class=\"mbr-col-sm-10 mbr-col-md-8 mbr-col-lg-5 btn-wrap\">\n                <span mbr-text class=\"phone-text mbr-fonts-style align-center\" mbr-theme-style=\"display-2\" data-app-selector=\".phone-text\">\n                    <a href=\"tel:015758082304\" class=\"text-primary\">+49(157)5808-2304</a></span>\n                <span mbr-text class=\"top-text mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showTop\" data-app-selector=\".top-text\">\n                    Rufen Sie uns